function somaMatriz(matriz) {
    let soma = 0;

    for (let i = 0; i < matriz.length; i++) {
        for (let j = 0; j < matriz[i].length; j++) {
            soma += matriz[i][j];
        }
    }

    return soma;
}

// Exemplo:
const matriz = [
    [10, 20],
    [30, 40],
    [50, 60]
];

console.log(somaMatriz(matriz)); // 210
